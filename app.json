{
  "expo": {
    "name": "Silvar",
    "slug": "silvar",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/silvar_1024.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "image": "./assets/images/silvar_1024.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/silvar_1024.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      },
      "bundleIdentifier": "com.silvar.app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/silvar_1024.png",
        "backgroundColor": "#000000"
      },
      "splash": {
        "image": "./assets/images/silvar_1024.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      },
      "package": "com.silvar.app",
      "versionCode": 1,
      "enableProguardInReleaseBuilds": true,
      "enableShrinkResources": true,
      "enableDexGuard": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/silvar_1024.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
